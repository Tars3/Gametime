<h1>Edit this Game!</h1>

<%= form_for @game do |f| %>

     <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>

    <div>
        <%= f.label :Boxart %>
        <%= f.text_field :image_url %>
    </div>

    <%= f.submit "Submit these changes!" %>

<% end %>

<%= link_to "Back to the list!", games_path, class: "btn btn-default" %>
