<h1>GAMETIME</h1>
<h2>Games</h2>

<ul>
  <% @games.each do |game| %>
    <li>
      <p><%= image_tag game.image_url%></p>
      <strong> <%= game.title %> </strong>
      <%= link_to "More info", game_path(game) %>
    </li><br>
  <% end %>
</ul>

<%= link_to 'Add Game', new_game_path %>
<br>
<%= link_to "List of Playtimes", playtimes_path %>
